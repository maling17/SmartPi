<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smartpi">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SmartPi">
        <activity android:name=".activity.sign.lupa_password.GantiLupaPasswordActivity"></activity>
        <activity android:name=".activity.sign.lupa_password.LupaPasswordAktivasiActivity" />
        <activity android:name=".activity.sign.lupa_password.LupaPasswordActivity" />
        <activity android:name=".activity.sign.SignUpActivity" />
        <activity android:name=".activity.sign.SignInPasswordActivity" />
        <activity android:name=".activity.KodeAktivasiActivity" />
        <activity android:name=".activity.sign.SignInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>